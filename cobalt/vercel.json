{
  "buildCommand": "cd api && npm install",
  "outputDirectory": "api",
  "installCommand": "npm install -g pnpm && pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/src/cobalt.js"
    }
  ],
  "functions": {
    "api/src/cobalt.js": {
      "runtime": "nodejs18.x"
    }
  }
}
